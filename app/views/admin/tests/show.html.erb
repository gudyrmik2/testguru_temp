<p>
  <strong><%= t('.title') %>:</strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level') %>:</strong>
  <%= @test.level %>
</p>

<p><%= link_to t('.new_question_link'), new_admin_test_question_path(@test) %></p>

<p><strong><%= t('.questions') %>:</strong></p>
<table>
  <tr>
    <th><%= t('.body') %></th>
    <th colspan="3"><%= t('.actions') %></th>
  </tr>
  <%= render @test.questions %>
</table>

<%= link_to t('.back_link'), admin_tests_path %> |
<%= link_to t('.edit_link'), edit_admin_test_path(@test) %>