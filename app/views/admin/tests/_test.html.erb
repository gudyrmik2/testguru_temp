<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('admin.tests.index.show_link'), admin_test_path(test) %></td>
  <td><%= link_to t('admin.tests.index.edit_link'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('admin.tests.index.delete_link'),
                  admin_test_path(test),
                  method: :delete,
                  data: { confirm: t('admin.tests.index.confirmation') } %>
  </td>
</tr>